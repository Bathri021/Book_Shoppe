@if (Book_Shoppe.Controllers.UserController.CurrentUser != null)
{

    try
    {
        <ul class="nav navbar-nav navbar-right">
            <li>
                @Html.ActionLink("Hello " + Book_Shoppe.Controllers.UserController.CurrentUser.Name + "!", "UserProfile", "User", routeValues: new { id = Book_Shoppe.Controllers.UserController.CurrentUser.UserID }, htmlAttributes: new { @class = "nav-link" })
            </li>
            <li>
                @Html.ActionLink("Log out", "LogOut", "User", null, new { @class = "nav-link" })
            </li>
        </ul>
    }
    catch (Exception)
    {

    }

}
else
{
    <ul class="nav navbar-nav navbar-right">
        <li>@Html.ActionLink("Register", "Register", "User", routeValues: null, htmlAttributes: new { @class = "nav-link", id = "registerLink" })</li>
        <li>@Html.ActionLink("Log in", "LogIn", "User", routeValues: null, htmlAttributes: new { @class = "nav-link", id = "loginLink" })</li>
    </ul>
}
